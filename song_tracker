from collections import deque

song_queue = deque()
song_count = {}

def add_song_play(timestamp, song_name):
	song_queue.append((timestamp, song_name))

	if song_name not in song_count:
		song_count[song_name] = 1
	else:
		song_count[song_name] += 1
	return song_queue, song_count

print(add_song_play(1, "Song A"))
print(add_song_play(2, "Song B"))
print(add_song_play(3, "Song C"))



